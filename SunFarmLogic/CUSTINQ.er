    BegProc *Entry Access(*Public)
 
 
        // KLIST "KeyMastL2" moved by Monarch to global scope.
    //********************************************************************
        DoUntil ( *IN03)
            *IN90 = "0"
            Write MSGSFC
            Write KEYS
            ExFmt SFLC 
            ClearMsgs()
    //--------------------------------------------------------------------
            Select
    // Exit
                When( *IN03)
                    *INLR = "1"
                    Leave
    // PageUp-RollDown
                When( *IN50)
                    SetLL L2RECFMT Key(CMNAME)
                    LoadSfl()
    // Work with spooled files
                When( *IN09)
                    CallD Pgm( "ACME.SunFarm.WWSPLF"  )
                When( SETNAME <> *Blanks )
                    MoveL SETNAME Name40 Fill(*Pad)
                    SetLL L2RECFMT Key(Name40)
                    LoadSfl()
    // PageDown-RollUp
                When( *IN51)
                    ReadBack()
                    LoadSfl()
                When( SETNAME <> *Blanks )
                    MoveL SETNAME Name40 Fill(*Pad)
                    SetLL L2RECFMT Key(Name40)
                    LoadSfl()
                Other

    // Process any individual Subfile record Option Selections

                    SetOff IndList( *IN30, *IN66 )
                    savrrn = sflrrn
                    pNumbers = 0
                    #pNbrs = 0
                    DoUntil ( *IN66)
                        ReadC SFL1 Eof(*IN66)
                        If( *Not *IN66)
                            Select
                                When( SFSEL = 10 ) // Print in batch.
                                    Chain SFL1 Key(sflrrn)
                                    #pNbrs = #pNbrs + 1
                                    pNumbers[ (#pNbrs) - 1 ] = SFCUSTNO
                                    MoveL "P" pTypes[#pNbrs-1]
                                    ClearSel()
                                When( SFSEL = 9 ) // Print online.
                                    SVCUSTNO = SFCUSTNO
                                    CallD Pgm( "ACME.SunFarm.CUSTPRTS"  )  LR(*IN88)
                                        DclParm SVCUSTNOa
                                    MID = "CST0006"
                                    MTX = " "
                                    CallD Pgm( "ACME.SunFarm.MSGLOD"  ) ParmList(#PLMSG)
                                    ClearSel()
                                When( SFSEL = 7 ) // Create sales rec.
                                    Chain SFL1 Key(sflrrn)
                                    #pNbrs = #pNbrs + 1
                                    pNumbers[ (#pNbrs) - 1 ] = SFCUSTNO
                                    MoveL "C" pTypes[#pNbrs-1]
                                    ClearSel()
                                When( SFSEL = 5 ) // Display delivery
                                    Chain SFL1 Key(sflrrn) //  addresses.
                                    CallD Pgm( "ACME.SunFarm.CUSTDELIV"  )  LR(*IN88)
                                        DclParm CpyFrom(SFCUSTNO) Name(pNumber)
                                    ClearSel()

                                When( SFSEL = 3 ) // Display sales and returns. 
                                    Chain SFL1 Key(sflrrn) 
                                    SalesInfo()

                                When( SFSEL = 11 ) // Maintainance.
                                    Chain SFL1 Key(sflrrn)
                                    ORDCUST = SFCUSTNO
                                    CallD Pgm( "ACME.SunFarm.ORDHINQ"  )
                                        DclParm ORDCUST
                                    ClearSel()

                                When( SFSEL = 2 ) // Maintainance.
                                    Chain SFL1 Key(sflrrn)
                                    RcdUpdate()
                            EndSl
                        EndIf
                    EndDo

                    If( #pNbrs > 0 ) //Are there any jobs
                        CallD Pgm( "ACME.SunFarm.CUSTSBMJOB"  )  LR(*IN88) // to submit to batch?
                            DclParm pNumbers
                            DclParm pTypes
                        MID = "CST0005"
                        MTX = #pNbrsAlf
                        CallD Pgm( "ACME.SunFarm.MSGLOD"  ) ParmList(#PLMSG)
                    EndIf

                    sflrrn = savrrn
            EndSl

        EndDo


    //*********************************************************************
    // UPDATE THE CUSTOMER RECORD
    //*********************************************************************

    EndProc
    BegSr RcdUpdate
        ClearSel()
        Move "U" AddUpd

        CMCUSTNO = SFCUSTNO
        LockRec = "N"
        CustChk()

        SFOLDNAME = CMNAME
        #SFNAME = CMNAME
        SFADDR1 = CMADDR1
        SFADDR2 = CMADDR2
        SFCITY = CMCITY
        SFSTATE = CMSTATE
        SFPOSTCODE = CMPOSTCODE
        MoveL CMFAX SFFAX
        SFPHONE = CMPHONE
        #$STATUS = CMACTIVE
        SFCONTACT = CMCONTACT
        SFCONEMAL = CMCONEMAL
        SFYN01 = CMYN01

        SetOn *IN40 // Place the cursor on
        SetOff IndList( *IN41, *IN42, *IN43 ) //  the name field.
        SetOff IndList( *IN44, *IN99 )

    //-------------------------------------------------------

        DoUntil ( *IN12)
            Write MSGSFC
            ExFmt CUSTREC 

            SetOff IndList( *IN40, *IN41, *IN42 )
            SetOff IndList( *IN43, *IN44 )

            ClearMsgs()
            *IN30 = "0"

            Select
                When( *IN03)
                    Leave

                When( *IN12)
                    Leave

    // Prompt
                When( *IN04)

                    If( CSRFLD = "#$STATUS" )
                        MoveL "SF" CSRFLD
                    EndIf
                    If( CSRFLD = "SFSTATE"  +
                        *Or ( CSRFLD = "SFSTATUS" ) )
                        CallD Pgm( "ACME.SunFarm.CUSTPRMPT"  )  LR(*IN88)
                            DclParm CSRFLD
                            DclParm pResult

                        If( pResult <> *Blanks )

                            If( CSRFLD = "SFSTATE" )
                                MoveL pResult SFSTATE
                            Else
                                MoveL pResult #$STATUS
                            EndIf
                        EndIf
                    Else
                        MID = "CST0004"
                        MTX = " "
                        CallD Pgm( "ACME.SunFarm.MSGLOD"  ) ParmList(#PLMSG)
                        SetOff *IN04
                    EndIf

                When( *IN06)
                    SVCUSTNO = CMCUSTNO
                    SetGT L1RECFMT_ReadOnly Key(*Hival)
                    ReadP L1RECFMT_ReadOnly // Removed Access(*NoLock)
                    CUSTOMERL1.StatusCode = CUSTOMERL1_ReadOnly.StatusCode
                    TEMPNO = CMCUSTNO + 100
                    Clear CUSTDS
                    CMCUSTNO = TEMPNO
                    Move "A" AddUpd
                    SFCUSTNO = TEMPNO
                    MoveL *Blanks #SFNAME
                    MoveL *Blanks SFADDR1
                    MoveL *Blanks SFADDR2
                    MoveL *Blanks SFCITY
                    MoveL *Blanks SFSTATE
                    MoveL *Blanks SFPOSTCODE
                    MoveL *Blanks SFFAX
                    MoveL *Blanks SFPHONE
                    MoveL *Blanks #$STATUS
                    MoveL *Blanks SFCONTACT
                    MoveL *Blanks SFCONEMAL
                    MoveL *Blanks SFYN01
                    *IN30 = "1"

    // Delete - leave sales hanging?

                When( *IN11)
                    LockRec = "Y"
                    CustChk()
                    If( *Not(*IN80) )
                        Delete From(L1RECFMT)
                    EndIf

                    Chain SFL1 Key(sflrrn)
                    Move *Blanks SFCSZ
                    SFNAME1 = "*** DELETED ***"
    // Set the color
                    *IN60 = "1"
                    Update SFL1
                    *IN60 = "0"
    // Delete msg
                    MID = "CST0003"
                    Move CMCUSTNO MTX
                    CallD Pgm( "ACME.SunFarm.MSGLOD"  ) ParmList(#PLMSG)
                    Leave

    // Add/update on the Enter key
                Other

                    If( AddUpd = "A" )
                        EditFlds()
                        If( *IN99) // Any errors?
                            Iterate
                        EndIf
                        UpdDbFlds()
                        Write L1RECFMT

    // Added message
                        MID = "CST0001"
                        Move CMCUSTNO MTX
                        CallD Pgm( "ACME.SunFarm.MSGLOD"  ) ParmList(#PLMSG)
                        Leave

    // Update the database
                    Else
                        EditFlds()
                        If( *IN99) // Any errors?
                            Iterate
                        EndIf
                        LockRec = "Y"
                        CustChk() //Reread the record.
                        UpdDbFlds()
                        Update L1RECFMT
                        Chain SFL1 Key(sflrrn)
                        SFNAME1 = CMNAME
                        Fix_CSZ()
                        Update SFL1

    // Updated message
                        MID = "CST0002"
                        MoveL CMCUSTNO MTX
                        CallD Pgm( "ACME.SunFarm.MSGLOD"  ) ParmList(#PLMSG)
                    EndIf

    // Re-open for the next update
                    LockRec = "N"
                    CustChk()
            EndSl

        EndDo

    EndSr

    //*********************************************************************
    //  EDIT THE SCREEN FIELDS
    //*********************************************************************

    BegSr EditFlds
        SetOff IndList( *IN40, *IN41, *IN42 ) //Clear all error inds
        SetOff IndList( *IN43, *IN44, *IN99 )

        Select
            When ( #SFNAME = *Blanks )
                SetOn IndList( *IN40, *IN99 )
                MID = "CST1001" // Blank field.
                MTX = "customer name"

            When ( SFADDR1 = *Blanks )
                SetOn IndList( *IN41, *IN99 )
                MID = "CST1001" // Blank field.
                MTX = "first address line"

            When ( SFCITY = *Blanks )
                SetOn IndList( *IN42, *IN99 )
                MID = "CST1001" // Blank field.
                MTX = "city"

            When ( SFSTATE = *Blanks )
                SetOn IndList( *IN43, *IN99 )
                MID = "CST1002" // Blank field.
                MTX = "state"

            When ( #$STATUS = *Blanks )
                SetOn IndList( *IN44, *IN99 )
                MID = "CST1002" //Blank field.
                MTX = "status"
        EndSl

        If( *IN99)
            CallD Pgm( "ACME.SunFarm.MSGLOD"  ) ParmList(#PLMSG)
        EndIf

    EndSr

    //*********************************************************************
    //  UPDATE THE DATABASE FIELDS
    //*********************************************************************

    BegSr UpdDbFlds
        CMCUSTNO = SFCUSTNO
        CMNAME = %Trim(#SFNAME)
        CMADDR1 = %Trim(SFADDR1)
        CMADDR2 = %Trim(SFADDR2)
        CMCITY = %Trim(SFCITY)
        CMSTATE = SFSTATE
        CMPOSTCODE = SFPOSTCODE
        MoveL SFFAX CMFAX
        CMPHONE = SFPHONE
        CMACTIVE = #$STATUS
        CMCONTACT = %Trim(SFCONTACT)
        CMCONEMAL = %Trim(SFCONEMAL)
        CMYN01 = %Trim(SFYN01)
    EndSr


    //*********************************************************************
    // Retrieve Calculated Sales Info
    //*********************************************************************

    BegSr SalesInfo
        ClearSel()

        CMCUSTNO = SFCUSTNO
        LockRec = "N"
        CustChk()

        CmCust# = CMCUSTNO
        Move CMCUSTNO CmCust#CH
        Move *Zeros SalesCh
        Move *Zeros ReturnsCh
        CallD Pgm( "ACME.SunFarm.CUSTCALC"  )  LR(*IN88)
            DclParm CmCust#CH
            DclParm SalesCh
            DclParm ReturnsCh

        SFNAME = CMNAME
        Move SalesCh SFSALES
        Move ReturnsCh SFRETURNS

        DoUntil ( *IN03 *Or ( *IN12) )
            LoadSalesAndReturns()
            ExFmt SALESREC 
        EndDo

        SetOff *IN03 // Reset the exit ind.
    EndSr


    //*********************************************************************
    //  Load Sfl Subroutine
    //*********************************************************************

    BegSr LoadSfl
        SetOff *IN61 //Start with green.
        *IN90 = "1" //Clear the subfile.
        Write SFLC

        SetOff IndList( *IN76, *IN90 ) //Display records.
        sflrrn = 0
        Read L2RECFMT Eof(*IN77)
        If STS02 <> *Zero
            SVS02 = STS02
        Else
            HRC02 = REC02
        EndIf

    //----------------------------------------------------------

        DoWhile ( *Not *IN77  /* EOF or full s/f. */ +
            *And ( sflrrn < CustomerSufilePageSize ) )

            SFCUSTNO = CMCUSTNO
            SFNAME1 = CMNAME
            Fix_CSZ()
            If( *IN61) //Save the color of
                SFCOLOR = "W"
            Else
                SFCOLOR = "G"
            EndIf

            sflrrn = sflrrn + 1
            Write SFL1
            *IN61 = (*Not *IN61) //Reverse the colour.

            Read L2RECFMT Eof(*IN77)
        EndDo

    // Any records found?

        If( sflrrn = 0 )
            sflrrn = 1
            Move *Blanks SFCUSTNO
            CMCUSTNO = 0
            MoveL "END OF FILE" SFNAME1
            Move *Blanks SFCSZ
            Write SFL1
        EndIf

    EndSr


    //*********************************************************************
    //  Read Backwards for a PageDown
    //*********************************************************************

    BegSr ReadBack

        SetOff IndList( *IN76, *IN77 )
        X = 0
        Chain SFL1 Key(1) //Get the top name and
        CMNAME = SFNAME1
        CMCUSTNO = SFCUSTNO
        Chain L2RECFMT Key(KeyMastL2)
        ReadP L2RECFMT Bof(*IN76)

        DoWhile ( *Not *IN76  /* EOF or full s/f. */ +
            *And ( X < CustomerSufilePageSize ) )
            X = X + 1
            ReadP L2RECFMT Bof(*IN76)
        EndDo

        If( *IN76) //Any records found?
            SetLL L2RECFMT Key(*LoVal)
        EndIf

    EndSr


    //*********************************************************************
    // Fix CSZ Subroutine
    //*********************************************************************

    BegSr Fix_CSZ
        SFCSZ = %TrimR(CMCITY) + ", " + CMSTATE +" " + CMPOSTCODE
    EndSr


    //*********************************************************************
    // CLEAR THE SELECTION NUMBER
    //*********************************************************************

    BegSr ClearSel
        SFSEL = 0
        *IN61 = (SFCOLOR = "W")
        Update SFL1
    EndSr


    //*********************************************************************
    // CLEAR THE MESSAGE QUEUE
    //*********************************************************************

    BegSr ClearMsgs
        CallD Pgm( "ACME.SunFarm.MSGCLR"  )
        MID = *Blank
    EndSr



    //*********************************************************************
    //     * Init Subroutine
    //*********************************************************************
    BegSr PROCESS_STAR_INZSR
        SetLL L2RECFMT Key(*Loval)
        LoadSfl()

        *IN75 = *On
        MID = *Blank
        @PGMQ = "*"
        CallD Pgm( "ACME.SunFarm.MSGCLR"  )
    EndSr
    
    BegSr LoadSalesAndReturns

        DclArray LastRecordedYearSales Like(CSSALES01) Dim(MonthsInYear)
        DclArray LastRecordedYearReturns Like(CSSALES01) Dim(MonthsInYear)
        DclFld LastRecordedYear Like(CSYEAR)
        DclFld month Type(Int16)
        
        CustChk()
        If *IN80 
           Return 
        EndIF
            
        *IN90 = "1" // Clear the subfile.
        Write SALESREC

        SalesSflrrn = 0
        LastRecordedYear = 0
        
        SetLL RCSMASTL1 Key(CMCUSTNO)
        ReadE From(RCSMASTL1) Key(CMCUSTNO) Eof(*IN03)
        
        DoWhile ( *Not *IN03)
            
            If CSYEAR *GE LastRecordedYear
                LastRecordedYear = CSYEAR
                
                If CSTYPE = SaleEvent 
                    LastRecordedYearSales = YearlySales
                Else
                    LastRecordedYearReturns = YearlySales
                EndIf
            EndIf
            
            ReadE From(RCSMASTL1) Key(CMCUSTNO) Eof(*IN03) // Read Next
        EndDo

        If LastRecordedYear *GT 0 
            For Index( month=0 ) To( MonthsInYear )  
                WriteSalesReturnsSubfileRecord( LastRecordedYear, LastRecordedYearSales[month], LastRecordedYearReturns[month])
            EndFor
        EndIf
        
        SetOff IndList( *IN76, *IN90 ) // Display records.
        Write SALESREC
    EndSr 
    
    BegSr WriteSalesReturnsSubfileRecord
        DclSrParm _year Like(YEAR)    
        DclSrParm _sales Like(SALES)
        DclSrParm _returns Like(RETURNS)
        
        YEAR = _year
        SALES = _sales 
        RETURNS = _returns
        
        SalesSflrrn = SalesSflrrn + 1
        Write SFL_SALESRETURNS
    EndSr
        
    //********************************************************************
    //   CHECK THE CUSTOMER NUMBER
    //********************************************************************
    BegSr CustChk
        If( LockRec = "N" )
            Chain L1RECFMT_ReadOnly Key(CMCUSTNO) NotFnd(*IN80) // Removed Access(*NoLock)
            CUSTOMERL1.StatusCode = CUSTOMERL1_ReadOnly.StatusCode
        Else
            Chain L1RECFMT Key(CMCUSTNO) NotFnd(*IN80)
        EndIf
    EndSr


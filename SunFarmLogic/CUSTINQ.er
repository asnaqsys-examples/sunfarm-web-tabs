        EndGet
    EndProp
    
    BegProp REC01 Type(*Integer) Len(4)
        BegGet
            LeaveSr CUSTOMERL1.RecNum 
        EndGet
    EndProp
    
    
/endregion

    
    BegSr Reset_#pNbrs
        #pNbrs = *Zeros
    EndSr
 
